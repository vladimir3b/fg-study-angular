          "options": {
            "browserTarget": "ngrx-basic-example:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngrx-basic-example:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngrx-basic-example:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngrx-basic-example/src/test.ts",
            "polyfills": "projects/ngrx-basic-example/src/polyfills.ts",
            "tsConfig": "projects/ngrx-basic-example/tsconfig.spec.json",
            "karmaConfig": "projects/ngrx-basic-example/karma.conf.js",
            "styles": ["projects/ngrx-basic-example/src/styles.scss"],
            "scripts": [],
            "assets": [
              "projects/ngrx-basic-example/src/favicon.ico",
              "projects/ngrx-basic-example/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["projects/ngrx-basic-example/tsconfig.app.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "basic-template"
}
